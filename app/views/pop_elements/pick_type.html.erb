<!-- Page Content -->
<div class="container">
  <div class="row">
    <div class="col-xs-2 col-md-9">
      <h1 style="margin-top:50px">Select Local POP to Order</h1>
    </div>
    <div class="col-xs-2 col-md-3"><!-- EMPTY DIV --></div>
    <div class="col-xs-6 col-md-8">
      <%= form_tag pop_elements_path, method: :get, class: 'form-group' do %>
        <%= select_tag(:element_type, options_for_select(
          [
            ["Home Delivery","Home Delivery"],
            ["Drive Thru","Drive Thru"],
            ["Loyalty","Loyalty"],
            ["Now Open","Now Open"],
            ["Opening Soon","Opening Soon"],
            ["Moved","Moved"],
            ["Closed","Closed"],
            ["Under Renovations","Under Renovations"]
          ]
        ), class: 'form-control', include_blank: true) %>

        <br>

        <div class="col-md-2">
          <%= submit_tag 'Select', class: 'btn btn-success' %>
        </div>
      <% end %>
    </div>
  </div>
</div>
<!-- /.container -->
